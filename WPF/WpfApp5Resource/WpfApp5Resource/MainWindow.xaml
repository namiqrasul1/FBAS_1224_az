<Window x:Class="WpfApp5Resource.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp5Resource"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button Height="{StaticResource primaryHeight}" Background="{StaticResource primaryColor}" Margin="10,20"></Button>
        <Button Height="{DynamicResource primaryHeight}" Background="{StaticResource primaryColor}" Margin="10,20"></Button>
        <Button Height="{StaticResource primaryHeight}" Background="{DynamicResource secondaryColor}" Margin="10,20"></Button>
        <Button Height="{StaticResource primaryHeight}" Background="{StaticResource primaryColor}" Margin="10,20" Click="Button_Click"></Button>
    </StackPanel>
</Window>
